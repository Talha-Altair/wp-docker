    server_name subscription.myfamistry.com;
    location / {
        proxy_pass http://localhost:7500;
    }
}

server {
    server_name vectorizer.myfamistry.com;
    location / {
        proxy_pass http://localhost:8500;
    }
}

server {
    server_name notification.myfamistry.com;
    location / {
        proxy_pass http://localhost:6500;
    }
}


server {
    server_name myfamistry.com;
    location / {
        proxy_pass http://localhost:9000;
    }
}

server {
    server_name  www.myfamistry.com;
    return       301 http://myfamistry.com$request_uri;
}

server {
    server_name php.myfamistry.com;
    location / {
        proxy_pass http://localhost:9080;
    }
}